{
  "dashboard": {
    "title": "RPS and Latency Dashboard",
    "tags": ["rps", "latency", "performance"],
    "timezone": "browser",
    "refresh": "10s",
    "schemaVersion": 38,
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "Requests Per Second",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "rate(service_rps_total[1m])",
            "legendFormat": "{{status}}",
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "label": "RPS"
          }
        }
      },
      {
        "id": 2,
        "title": "Request Latency (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(service_latency_seconds_bucket[5m]))",
            "legendFormat": "p50",
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(service_latency_seconds_bucket[5m]))",
            "legendFormat": "p95",
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(service_latency_seconds_bucket[5m]))",
            "legendFormat": "p99",
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "label": "Latency"
          }
        }
      }
    ]
  }
}